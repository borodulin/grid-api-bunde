{
    "name": "borodulin/grid-api-bundle",
    "description": "Grid Api Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Borodulin",
            "email": "borodulin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "symfony/http-kernel": "^5.2",
        "symfony/property-access": "^5.2",
        "symfony/property-info": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/yaml": "^5.2",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^5.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Borodulin\\Bundle\\GridApiBundle\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.2"
        }
    }
}
